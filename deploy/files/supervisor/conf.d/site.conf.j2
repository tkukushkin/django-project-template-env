[program:site]
command = /home/{{ user }}/.env/bin/python /home/{{ user }}/.env/bin/gunicorn --workers=2 --log-file - --access-logfile - --bind=unix:/home/{{ user }}/run/site.sock project.wsgi:application
directory = /home/{{ user }}/src/
autostart = true
autorestart = true
redirect_stderr = true
stdout_logfile = /home/{{ user }}/log/site.log
